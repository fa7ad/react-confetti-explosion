{
  "name": "@fa7ad/react-confetti-explosion",
  "version": "2.1.4-1",
  "private": false,
  "author": "Ethan Herr <herrethan@gmail.com>",
  "description": "A lightweight css-animation based confetti exploder for React",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:demo": "vite --config vite-demo.config.ts build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "preview": "vite preview",
    "test": "vitest",
    "prepack": "pnpm run build"
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "lodash.round": "^4.0.4",
    "react-jss": "^10.10.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.round": "^4.0.7",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/testing-library__jest-dom": "^5.14.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.0",
    "vite-plugin-dts": "^3.3.0",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "confetti",
    "css animation",
    "particles",
    "user reward",
    "ui delight"
  ],
  "exports": {
    ".": {
      "import": "./dist/react-confetti-explosion.es.js",
      "require": "./dist/react-confetti-explosion.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ]
}
